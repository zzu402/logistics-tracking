public class UserPostVo {
	
	private String orgId;
	//组织名称
	private String orgName;
	
	private List<UserRoleVo> userRoleVos;
	
	
	
	
	public List<UserRoleVo> getUserRoleVos() {
		return userRoleVos;
	}
	public void setUserRoleVos(List<UserRoleVo> userRoleVos) {
		this.userRoleVos = userRoleVos;
	}
	public String getOrgId() {
		return orgId;
	}
	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}
	public String getOrgName() {
		return orgName;
	}
	public void setOrgName(String orgName) {
		this.orgName = orgName;
	}
	@Override
	public String toString() {
		return "UserPostVo [orgId=" + orgId + ", orgName=" + orgName + ", userRoleVo=" + userRoleVos + "]";
	}

	
	

