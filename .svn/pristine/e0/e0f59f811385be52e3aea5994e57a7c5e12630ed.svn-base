package com.htkfood.entity;

import java.util.List;

import com.htkfood.entity.vo.UserPostVo;

/*
 * 用户信息实体
 */

public class UserEntity {
	//用户id
	private String userId;
	//用户名称
	private String name;
	//考勤号
	private String attenceNo;
	//职位信息
	private List<UserPostVo>  userPostVos;
	
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAttenceNo() {
		return attenceNo;
	}
	public void setAttenceNo(String attenceNo) {
		this.attenceNo = attenceNo;
	}
	public List<UserPostVo> getUserPostVos() {
		return userPostVos;
	}
	public void setUserPostVos(List<UserPostVo> userPostVos) {
		this.userPostVos = userPostVos;
	}
	@Override
	public String toString() {
		return "UserEntity [userId=" + userId + ", name=" + name + ", attenceNo=" + attenceNo + ", userPostVos="
				+ userPostVos + "]";
	}
	
	
}
